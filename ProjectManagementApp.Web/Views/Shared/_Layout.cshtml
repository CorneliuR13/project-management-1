@using System.Web.Optimization;

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>

    <!-- App favicon -->
    <link rel="shortcut icon" href="~/Content/assets/images/favicon.ico">

    <!-- plugins -->
    <link href="~/Content/assets/libs/flatpickr/flatpickr.min.css" rel="stylesheet" type="text/css" />

    <!-- App css -->
    <link href="~/Content/assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bs-default-stylesheet" />

    @Styles.Render("~/bundles/app/css")

    <link href="~/Content/assets/css/bootstrap-dark.min.css" rel="stylesheet" type="text/css" id="bs-dark-stylesheet" disabled />
    <link href="~/Content/assets/css/app-dark.min.css" rel="stylesheet" type="text/css" id="app-dark-stylesheet" disabled />

    <!-- icons -->
    <link href="~/Content/assets/css/icons.min.css" rel="stylesheet" type="text/css" />



</head>
<body>
    @model ProjectManagementApp.Web.Models.UserData

    <div class="navbar-custom">
        @Html.Partial("_Header")
    </div>
    <div class="left-side-menu">
        @Html.Partial("_SideMenu")
    </div>

    <div class="content-page">
        @RenderBody()
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    <script src="~/Content/assets/js/vendor.min.js"></script>

    <!-- optional plugins -->
    <script src="~/Content/assets/libs/moment/min/moment.min.js"></script>
    <script src="~/Content/assets/libs/apexcharts/apexcharts.min.js"></script>
    <script src="~/Content/assets/libs/flatpickr/flatpickr.min.js"></script>

    <!-- page js -->
    <script src="~/Content/assets/js/pages/dashboard.init.js"></script>

    <!-- App js -->
    <script src="~/Content/assets/js/app.min.js"></script>
</body>
</html>